{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "npx prisma generate",
    "migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:create": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "migrate:deploy": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "prisma-zod": "cd pre-build && npm run build && cd .. &&  npx prettier src/utils/prismaTypes --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.23.4",
    "@prisma/client": "^5.4.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
